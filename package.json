{
  "name": "domapp",
  "version": "1.0.0",
  "description": "Document Management App",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --watch server --exec babel-node -- ./server/bin/index.js",
    "test": "npm run server-test",
    "server-test": "npm run refill-testdb && NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/.bin/mocha --timeout 15000 --compilers js:babel-core/register server/test/**/*.test.js",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "reset-testdb": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "seed-testdb": "NODE_ENV=test sequelize db:seed:all",
    "refill-testdb": "npm run reset-testdb && npm run seed-testdb",
    "build-client": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.babel.js",
    "build-server": "babel server -d dist/server --copy-files --ignore test.js",
    "reset-proddb": "NODE_ENV=production ./node_modules/.bin/sequelize db:migrate:undo:all && NODE_ENV=production ./node_modules/.bin/sequelize db:migrate",
    "pre-start": "npm run reset-proddb && NODE_ENV=production ./node_modules/.bin/sequelize db:seed:all",
    "clean": "rm -R dist && mkdir dist",
    "build": "npm run clean && npm run build-client && npm run build-server",
    "ci-build": "npm run pre-start && npm run build",
    "start": "node dist/server/bin/www.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-dhaniabelega/domapp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-dhaniabelega/domapp/issues"
  },
  "homepage": "https://github.com/andela-dhaniabelega/domapp#readme",
  "dependencies": {
